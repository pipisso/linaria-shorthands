{
  "name": "linaria-shorthands",
  "version": "0.2.0",
  "license": "MIT",
  "author": {
    "name": "Levin Uncu",
    "email": "levin@uncu.de"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "typecheck": "tsc",
    "build": "unbuild",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "lint": "eslint \"src/**/*.ts\" && pnpm typecheck",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pipisso/linaria-shorthands"
  },
  "bugs": {
    "url": "https://github.com/pipisso/linaria-shorthands/issues"
  },
  "homepage": "https://github.com/pipisso/linaria-shorthands/README.md",
  "packageManager": "pnpm@8.6.12",
  "sideEffects": false,
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "csstype": "^3.1.2",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1"
  }
}
